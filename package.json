{
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@7.25.0",
  "author": "Creasi Developers <developers@creasi.co>",
  "engines": {
    "node": "14.x || 16.x"
  },
  "scripts": {
    "build": "vite build && vite build --ssr",
    "dev": "vite",
    "prepare": "husky install scripts/husky"
  },
  "dependencies": {
    "@inertiajs/vue3": "^1.0.0",
    "@sentry/browser": "^7.31.1",
    "@sentry/tracing": "^7.31.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/server-renderer": "^3.2.45",
    "alpinejs": "^3.11.1",
    "axios": "^1.2.3",
    "date-fns": "^2.29.3",
    "sweetalert2": "^11.4.23",
    "vue": "^3.2.45",
    "vue-i18n": "9.3.0-beta.16"
  },
  "devDependencies": {
    "@antfu/eslint-config-vue": "^0.27.0",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@fontsource/nunito": "^4.5.11",
    "@types/alpinejs": "^3.7.1",
    "@types/node": "^18.11.18",
    "cross-env": "^7.0.3",
    "eslint": "^8.32.0",
    "husky": "^8.0.3",
    "laravel-vite-plugin": "^0.7.3",
    "lint-staged": "^13.1.0",
    "typescript": "^4.9.4",
    "vite": "^3.2.5",
    "vite-plugin-windicss": "^1.8.10",
    "windicss": "^3.5.6"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{json,js,ts,vue}": [
      "eslint --fix"
    ],
    "{app,config,database,resources,tests}/**/*.php": [
      "php vendor/bin/pint --preset laravel"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "browser": true
    },
    "extends": "@antfu/vue",
    "rules": {
      "vue/no-reserved-component-names": "off"
    }
  }
}
